{
  "editor.formatOnSave": true,
  "editor.formatOnPaste": false,
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "scss.lint.unknownAtRules": "ignore",
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "[html,svg]": {
    "editor.formatOnSaveMode": "file",
    "editor.defaultFormatter": "HookyQR.beautify"
  },
  "material-icon-theme.folders.associations": {
    "access": "secure",
    "jsonrpc": "public"
  },
  "material-icon-theme.files.associations": {
    "index.ts": "tree",
    "main.ts": "tree",
    "mod.ts": "tree",
    "config.ts": "settings",
    "*.access.ts": "key",
    "*.database.ts": "database",
    "*.collection.ts": "mxml",
    "*.controller.ts": "makefile",
    "*.events.ts": "lib",
    "*.method.ts": "makefile",
    "*.methods.ts": "makefile",
    "*.mock.ts": "hardhat",
    "*.entity.ts": "database",
    "*.form.ts": "log",
    "*.gateway.ts": "parcel",
    "*.module.ts": "robot",
    "*.projector.ts": "apiblueprint",
    "*.role.ts": "playwright",
    "*.service.ts": "console",
    "*.validator.ts": "key",
    "*.component.tsx": "virtual",
    "*.modal.tsx": "opam",
    "*.view.tsx": "virtual",
  },
  "files.exclude": {
    "**/.turbo": true,
    "**/node_modules": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true
  },
  "cSpell.words": [
    "birthdayresearch",
    "fastify",
    "headlessui",
    "IDBP",
    "indexeddb",
    "inmemory",
    "kodemon",
    "mingo",
    "nestjs",
    "nvmrc",
    "pako",
    "pannable",
    "pino",
    "quicktype",
    "reactflow",
    "rfdc",
    "RÃ¸dvik",
    "tailwindcss",
    "tsbuildinfo",
    "untar",
    "valkyr",
    "viewports",
    "zoomable"
  ],
  "typescript.tsdk": "node_modules/typescript/lib"
}